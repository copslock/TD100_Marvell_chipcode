# PXA Platform Support
snd-soc-pxa2xx-objs := pxa2xx-pcm.o
snd-soc-pxa2xx-ac97-objs := pxa2xx-ac97.o
snd-soc-pxa2xx-i2s-objs := pxa2xx-i2s.o
snd-soc-pxa-ssp-objs := pxa-ssp.o
snd-soc-pxa3xx-ssp-objs := pxa3xx-ssp.o
snd-soc-pxa910-objs := pxa910-squ.o pxa3xx-pcm.o
snd-soc-pxa910-platform-objs := pxa910-platform.o
snd-soc-pxa95x-pcm-ssp-objs := pxa95x-pcm-ssp.o
snd-soc-pxa95x-abu-objs := pxa95x-abu.o
snd-soc-pxa95x-pcm-abu-objs := pxa95x-pcm-abu.o
snd-soc-mmp3-objs := mmp2-sspa.o mmp2-squ.o

obj-$(CONFIG_SND_PXA2XX_SOC) += snd-soc-pxa2xx.o
obj-$(CONFIG_SND_PXA2XX_SOC_AC97) += snd-soc-pxa2xx-ac97.o
obj-$(CONFIG_SND_PXA2XX_SOC_I2S) += snd-soc-pxa2xx-i2s.o
obj-$(CONFIG_SND_PXA_SOC_SSP) += snd-soc-pxa-ssp.o
obj-$(CONFIG_SND_PXA3XX_SOC_SSP) += snd-soc-pxa3xx-ssp.o
obj-$(CONFIG_SND_PXA910_SOC) += snd-soc-pxa910.o
obj-$(CONFIG_SND_PXA910_SOC) += snd-soc-pxa910-platform.o
obj-$(CONFIG_SND_PXA95X_SOC) += snd-soc-pxa95x-pcm-ssp.o
obj-$(CONFIG_SND_PXA95X_SOC_ABU) += snd-soc-pxa95x-abu.o
obj-$(CONFIG_SND_PXA95X_SOC_ABU) += snd-soc-pxa95x-pcm-abu.o
obj-$(CONFIG_SND_MMP3_SOC) += snd-soc-mmp3.o

