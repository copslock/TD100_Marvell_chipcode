/* We need the GLOBAL mutex which is not supported by BIONIC
 * Implement the "pseudo-mutex" by Exclusive-file open.
 * There is NO blocking but error so apply the usleep()
 */

static int mutex_mrd = -1;

static mrd_mutex_lock(void)
{
 do {
  mutex_mrd = open("/tmp/MRD_mutex.bin", 00000100 | 00000200);
  if (mutex_mrd >= 0)
   break;
  usleep(100000);
 } while (mutex_mrd < 0);
}

static mrd_mutex_unlock(void)
{
 if (mutex_mrd >= 0) {
  close(mutex_mrd);
  mutex_mrd = -1;
  unlink("/tmp/MRD_mutex.bin");
 }
}

/***********************************************************************
* Function: flashEraseBlockMTD	 				                       *
************************************************************************
* Description: Erase a MTD block									   *
*              				                                           *
* Parameters: none                                                     *
*              														   *
*                                                                      *
* Return value: none                                                   *
*                                                                      *
* Notes:                                                               *
***********************************************************************/
static int flashEraseBlockMTD(char *dev, unsigned int flash_offset)
{
 mtd_info_t meminfo;
 int fd;
 erase_info_t erase;
 int ret_val = 0;

 fd = open(dev, 00000002);
 if (fd == -1) {
  ret_val = -1;
  goto clean_up;
 }

 if (ioctl(fd, (((2U) << (((0 +8)+8)+14)) | ((('M')) << (0 +8)) | (((1)) << 0) | (((((sizeof(struct mtd_info_user) == sizeof(struct mtd_info_user[1]) && sizeof(struct mtd_info_user) < (1 << 14)) ? sizeof(struct mtd_info_user) : __invalid_size_argument_for_IOC))) << ((0 +8)+8))), &meminfo) != 0) {
  ret_val = -2;
  goto clean_up;
 }

 /* Initialize to write size */
 g_block_sz = meminfo.writesize;

 erase.start = flash_offset;
 erase.length = meminfo.erasesize;
 if (ioctl(fd, (((1U) << (((0 +8)+8)+14)) | ((('M')) << (0 +8)) | (((2)) << 0) | (((((sizeof(struct erase_info_user) == sizeof(struct erase_info_user[1]) && sizeof(struct erase_info_user) < (1 << 14)) ? sizeof(struct erase_info_user) : __invalid_size_argument_for_IOC))) << ((0 +8)+8))), &erase) != 0) {
  ret_val = -3;
  goto clean_up;
 }

clean_up:
 if (fd != -1)
  close(fd);
 if (ret_val != 0)
  eeh_log(5, "flashEraseBlockMTD(): Error - ret_val=%d\n", ret_val);

 return ret_val;
}

