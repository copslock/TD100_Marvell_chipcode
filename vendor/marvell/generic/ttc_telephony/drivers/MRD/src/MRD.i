/***********************************************************************
* Function: MRDParseHeader						                       *
************************************************************************
* Description: Adds a file to the MRD image							   *
*              				                                           *
* Parameters: MRD_filename - Internal MRD filename                     *
*            				                                           *
* Return value: none                                                   *
*                                                                      *
* Notes:                                                               *
***********************************************************************/
static MRDErrorCodes MRDParseHeader(unsigned char *MRD_file_buffer)
{
 UINT32 image_checksum;
 MRD_header_t *pMRD_hdr;
 UINT8 *pMrd_start_addr = ((void *)0);
 UINT8 *pMrd_end_addr = ((void *)0);

 pMRD_hdr = (MRD_header_t *)MRD_file_buffer;

 eeh_log(5, "MRD header\n");
 eeh_log(5, "==========\n");
 eeh_log(5, "validBufferStamp=0x%x\n", pMRD_hdr->MRD_id.validBufferStamp);
 eeh_log(5, "bufCheckSum=0x%x\n", pMRD_hdr->MRD_id.bufCheckSum);
 //DPRINTF("reserve=0x%x\n", pMRD_hdr->reserve);
 //DPRINTF("reserve2=0x%x\n", pMRD_hdr->reserve2);
 eeh_log(5, "version=0x%x\n", pMRD_hdr->version);

 if (0xCAFECAFE != pMRD_hdr->MRD_id.validBufferStamp)
  return MRD_INVALID_HEADER_ERROR;

 eeh_log(5, "MRD: Calculating checksum\n");

 pMrd_start_addr = (UINT8 *)MRD_file_buffer;
 pMrd_end_addr = (UINT8 *)((UINT8 *)MRD_file_buffer + 0x20000);
 image_checksum = MRDChecksumCalc((UINT32 *)pMrd_start_addr,
          (UINT32 *)pMrd_end_addr);

 eeh_log(5, "MRD: Done. Calculated checksum=0x%x\n",image_checksum);

 if (image_checksum != pMRD_hdr->MRD_id.bufCheckSum) {
  eeh_log(5, "MRD ERROR: Calculated checksum does not match image checksum\n");
  eeh_log(5, "Calculated checksum=0x%x\n", image_checksum);
  eeh_log(5, "Image checksum=0x%x\n", pMRD_hdr->MRD_id.bufCheckSum);
  return MRD_INVALID_CHECKSUM_ERROR;
 }

 return MRD_NO_ERROR;
}


