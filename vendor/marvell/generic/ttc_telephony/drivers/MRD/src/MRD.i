/***********************************************************************
* Function: MRDRawBlockWrite					                       *
************************************************************************
* Description: Writes a raw MRD block to flash	 					   *
*              				                                           *
* Parameters: none                                                     *
*              														   *
*                                                                      *
* Return value: none                                                   *
*                                                                      *
* Notes:                                                               *
***********************************************************************/
static MRDErrorCodes MRDRawBlockWrite(void)
{
 int len;
 MRDErrorCodes ret_val;
 char shell_cmd[512];
 FILE *file;
 UINT32 crc;
 UINT32 *pMRD_end;
 MRD_header_t *pMRD_hdr;
 UINT32 num_blocks_skip;
 UINT32 num_blocks_to_read;
 int ret;
 UINT32 tmp_crc=0;
 char local_buf[200];
 unsigned char *temp_MRD_file_buffer=((void *)0);
 int try_num = 0;

 /* Allocate temp MRD file buffer */
 temp_MRD_file_buffer = malloc((128*1024));
 if (temp_MRD_file_buffer == ((void *)0))
  return MRD_NO_MEMORY_ERROR;

 /* Update checksum */
 pMRD_end = (UINT32 *)((UINT32)MRD_file_buffer + 0x20000);
 crc = MRDChecksumCalc((UINT32 *)MRD_file_buffer, pMRD_end);

 pMRD_hdr = (MRD_header_t *)MRD_file_buffer;
 pMRD_hdr->MRD_id.bufCheckSum = crc;

 tmp_crc = crc;

 /* Write MRD file snapshot from DDR to the RAM disk */
 file = fopen("/tmp/MRD.bin", "wb");
 if (((void *)0) == file)
 {
  ret_val = MRD_FILE_NOT_FOUND_ERROR;
  goto ERROR_EXIT;
 }

 len = fwrite(MRD_file_buffer, 1, (128*1024), file);
 fclose(file);

 if (0x20000 != len)
 {
  ret_val = MRD_FILE_WRITE_ERROR;
  goto ERROR_EXIT;
 }

 num_blocks_skip = g_mrd_block_offset / g_block_sz;
 num_blocks_to_read = ((128*1024) / g_block_sz);

TRY_AGAIN:
 eeh_log(5, "MRD: Writing to the MRD TRY #%d\n", try_num);
 /*#define MRD_RAW_BLOCK_READ_WRITE_CMD	"dd if=%s of=%s bs=%d count=%d skip=%d"*/
 len = snprintf(shell_cmd,
     512,
     g_raw_block_write_cmd,
     "/tmp/MRD.bin",
     g_mrd_mtd_partition,
     g_block_sz,
     num_blocks_to_read,
     num_blocks_skip);

 if (len > 512)
 {
  ret_val = MRD_NO_ERROR;
  goto ERROR_EXIT;
 }

 /* initiate command */
 eeh_log(5, "MRD: Writing to the MRD using the following commandline:\n%s\n", shell_cmd);
 ret_val = system(shell_cmd);
 eeh_log(5, "MRD: Write return val %d\n", ret_val);
 sync();
 len=0;
 file = fopen("/tmp/MRD_tmp_retval.bin", "r");
 if (((void *)0) == file)
 {
  ret_val = MRD_VERIFY_ERROR1;
  eeh_log(5, "MRD: Error1 #%d\n", ret_val);
 }
 else
 {
  len = fread(local_buf,1,sizeof(local_buf),file);
  fclose(file);
 }
 if(len <=0)
 {
  ret_val = MRD_VERIFY_ERROR2;
  eeh_log(5, "MRD: Error2 #%d\n", ret_val);
 }
 else
 {
  if(local_buf[0] == '0')
  {
   ret_val=MRD_NO_ERROR;
   eeh_log(5, "MRD: Error3 #%d\n", ret_val);
  }
  else //dd operation failed
  {
   ret_val=MRD_VERIFY_ERROR2;
   eeh_log(5, "MRD: Error4 #%d\n", ret_val);
  }
 }

 memcpy(temp_MRD_file_buffer, MRD_file_buffer, (128*1024));
 memset((char*)MRD_file_buffer, 0x00, (128*1024));

 ret_val = MRDRawBlockRead("/tmp/MRD_verify.bin");
 if(ret_val != 0)
 {
  ret_val= MRD_VERIFY_ERROR3;
  eeh_log(5, "MRD: Error5 #%d\n", ret_val);
 }

 crc = MRDChecksumCalc((UINT32 *)MRD_file_buffer, pMRD_end);
 if(crc != tmp_crc)
 {
  sprintf(shell_cmd, "cp %s %s ; cp %s %s ; dmesg > /data/MRD_log.txt",
   "/tmp/MRD.bin",
   "/data/MRD.bin",
   "/tmp/MRD_verify.bin",
   "/data/MRD_verify.bin");
  system(shell_cmd);
  ret_val=MRD_VERIFY_ERROR4;
  eeh_log(5, "MRD: Error6 #%d\n", ret_val);
  // Restore from backup copy
  memcpy(MRD_file_buffer, temp_MRD_file_buffer, (128*1024));
  if (try_num++ > 3) goto ERROR_EXIT;
  //add erase operation in case of error
  eeh_log(5, "MRD: erase operation in case of error\n");
  ret = g_erase_block_func(g_mrd_mtd_partition, 0x00);
  eeh_log(5, "MRD: erase operation retval-%d\n",ret);
  goto TRY_AGAIN;
 }
 else
  ret_val=MRD_NO_ERROR;

 /* Remove the temporary MRD.bin file !!! */
 //sprintf(shell_cmd, "rm %s", MRD_TMP_FILENAME);
 //system(shell_cmd);
ERROR_EXIT:
 free(temp_MRD_file_buffer);
 eeh_log(5, "MRD: Exit with Error #%d\n", ret_val);
 return ret_val;
}

/***********************************************************************
* Function: MRDRawBlockRead						                       *
************************************************************************
* Description: Writes a raw MRD block to flash	 					   *
*              				                                           *
* Parameters: none                                                     *
*              														   *
*                                                                      *
* Return value: none                                                   *
*                                                                      *
* Notes:                                                               *
***********************************************************************/
static MRDErrorCodes MRDRawBlockRead(char *filename)
{
 int len;
 int ret_val;
 char shell_cmd[512];
 UINT32 num_blocks_skip;
 UINT32 num_blocks_to_read;
 FILE *file;
 char local_buf[200];

 if (filename == ((void *)0))
  return MRD_FILE_NOT_FOUND_ERROR;

 num_blocks_skip = g_mrd_block_offset / g_block_sz;
 num_blocks_to_read = (128*1024) / g_block_sz;
 eeh_log(5, "num_blocks_skip=%d, num_blocks_to_read=%d, g_block_sz=%d\n", num_blocks_skip, num_blocks_to_read, g_block_sz);
 /*#define MRD_RAW_BLOCK_READ_WRITE_CMD	"dd if=%s of=%s bs=%d count=%d skip=%d"*/
 len = snprintf(shell_cmd,
     512,
     g_raw_block_read_cmd,
     g_mrd_mtd_partition,
     filename,
     g_block_sz,
     num_blocks_to_read,
     num_blocks_skip);

 if (len > 512)
  return MRD_ERROR;

 /* initiate command */
 eeh_log(5, "MRD: reading from the MRD using the following commandline:\n%s\n", shell_cmd);
 ret_val = system(shell_cmd);

 /* the access permission of the file generated by dd command is 1100 ---xr----T
	 * in order to read it, we need to change it to 700
	 */
 chmod(filename, 00700);

 sync();

 file = fopen("/tmp/MRD_tmp_retval.bin", "r");
 if (((void *)0) == file)
  return MRD_FILE_NOT_FOUND_ERROR;

 len = fread(local_buf,1,sizeof(local_buf),file);
 fclose(file);
 if(len <=0)
  ret_val = MRD_VERIFY_ERROR5;
 else {
  if(local_buf[0] == '0')
   ret_val=MRD_NO_ERROR;
  else
   ret_val=MRD_VERIFY_ERROR6;
 }

 /* read file into buffer */
 file = fopen(filename, "rb");
 if (((void *)0) == file) {
  return MRD_VERIFY_ERROR7;
 }

 len = fread(MRD_file_buffer, 1, (128*1024), file);
 if (len == 0) {
  fclose(file);
  return MRD_FILE_READ_ERROR;
 }

 fclose(file);

 /* Remove the temporary MRD.bin file !!! */
 //sprintf(shell_cmd, "rm %s", filename);
 //system(shell_cmd);

 return (ret_val == 0) ? MRD_NO_ERROR : MRD_ERROR;
}
