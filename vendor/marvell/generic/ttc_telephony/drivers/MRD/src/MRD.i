*                                                                      *
* Notes:                                                               *
***********************************************************************/
MRDErrorCodes MRDFlashTypeInit(void)
{
 switch(get_flash_type())
 {
 case FLASH_TYPE_NAND:
  {
   int mtd_number = mtd_name_to_number("imei");
   if(mtd_number < 0)
    return MRD_FLASH_TYPE_ERROR;
   sprintf(g_mrd_mtd_partition, "/dev/block/mtdblock%d", mtd_number);
   g_raw_block_read_cmd = "dd if=%s of=%s bs=%d count=%d seek=%d ; echo $? > /tmp/MRD_tmp_retval.bin";
   g_raw_block_write_cmd = "dd if=%s of=%s bs=%d count=%d seek=%d ; echo $? > /tmp/MRD_tmp_retval.bin";
   g_block_sz = (1024);
   g_mrd_block_offset = 0x0;
   g_erase_block_func = flashEraseBlockMTD;
  }
  break;

 case FLASH_TYPE_EMMC:
  {
   sprintf(g_mrd_mtd_partition, "/dev/block/mmcblk0p%d", 3);
   g_raw_block_read_cmd = "dd if=%s of=%s bs=%d count=%d skip=%d ; echo $? > /tmp/MRD_tmp_retval.bin";
   g_raw_block_write_cmd = "dd if=%s of=%s bs=%d count=%d seek=%d ; echo $? > /tmp/MRD_tmp_retval.bin";
   g_block_sz = (512);
   g_mrd_block_offset = 0x0;
   g_erase_block_func = flashEraseBlockMMC;
  }
  break;

 default:
  return MRD_FLASH_TYPE_ERROR;
 }

 return MRD_NO_ERROR;
}


/***********************************************************************
* Function: MRDInit								                       *
************************************************************************
* Description: Initializes the MRD module							   *
*              				                                           *
* Parameters: none                                                     *
*              														   *
*                                                                      *
* Return value: none                                                   *
*                                                                      *
* Notes:                                                               *
***********************************************************************/
MRDErrorCodes MRDInit(void)
{
 MRDErrorCodes ret_val;
 MRD_header_t mrd_header;
 MRD_entry_header_t MRD_end_hdr;

 if (0 != initialized)
  return MRD_ALREADY_INITIALIZED_ERROR;

 mrd_mutex_lock();

 ret_val = MRDFlashTypeInit();
 if (ret_val != MRD_NO_ERROR) {
  mrd_mutex_unlock();
  return ret_val;
 }

 /* Allocate MRD file buffer */
 MRD_file_buffer = malloc((128*1024));
 if (MRD_file_buffer == ((void *)0)) {
  mrd_mutex_unlock();
  return MRD_NO_MEMORY_ERROR;
 }
 /* Read MRD block from flash */
 ret_val = MRDRawBlockRead("/tmp/MRD.bin");
 if (ret_val != MRD_NO_ERROR) {
  mrd_mutex_unlock();
  return ret_val;
 }
 /* Parse MRD header - is valid MRD? */
 ret_val = MRDParseHeader(MRD_file_buffer);

 if (ret_val == MRD_NO_ERROR) {
  eeh_log(5, "MRD read from flash successfully\n");
  initialized = 1;
 } else {
  /* No MRD signature in header? assume file is empty */
  if (ret_val == MRD_INVALID_HEADER_ERROR) {
   /* Shuki: TODO - Add a journal entry here !!!! */
   /* Create a new header */
   eeh_log(5, "Error: MRD read from flash has an invalid header\n");
   eeh_log(5, "Creating a new MRD header...\n");
   MRDInitNewHeader(&mrd_header);
   memset(MRD_file_buffer, 0x00, (128*1024));
   memcpy(MRD_file_buffer, &mrd_header, sizeof(MRD_header_t));
   eeh_log(5, "Done.\n");

   eeh_log(5, "Creating a new MRD end file...\n");
   memset(&MRD_end_hdr, 0x00, sizeof(MRD_entry_header_t));
   MRD_end_hdr.entry_type = 0xDADADADA;
   MRD_end_hdr.header_checksum = MRDFileHeaderChecksumCalc(&MRD_end_hdr);
   memcpy(((UINT8 *)MRD_file_buffer + sizeof(mrd_header)),
       &MRD_end_hdr,
       sizeof(MRD_entry_header_t));

   eeh_log(5, "Done.\n");

   /* return no error to the user */
   ret_val = MRD_NO_ERROR;
   initialized = 1;
  } else {
   free(MRD_file_buffer);
   mrd_mutex_unlock();
  }
 }

 return ret_val;
}


