    <application android:icon="@drawable/icon"
        android:label="@string/app_name">
        <activity android:name=".UsbSettingListActivity"
            android:label="@string/app_name" android:process="com.android.phone">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CellBroadcastActivity"
            android:label="@string/CBS_setting" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name="UsbMenu">
            <intent-filter>
                <action android:name="com.marvell.android.USB_MENU_SHOW"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity>
        <activity android:name="UsbMenu_3">
            <intent-filter>
                <action android:name="com.marvell.android.USB_MENU_SHOW_3"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity>
        <activity android:name=".ClassBandSetting"
            android:label="@string/band_setting" android:process="com.android.phone">
            <intent-filer>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filer>
        </activity>
        <activity android:name=".GsmBandSetting" android:label="@string/gsm_band_setting"
            android:process="com.android.phone">
            <intent-filer>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filer>
        </activity>
        <activity android:name=".WcdmaBandSetting"
            android:label="@string/w_band_setting" android:process="com.android.phone">
            <intent-filer>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filer>
        </activity>
        <activity android:name=".TdBandSetting" android:label="@string/td_band_setting"
            android:process="com.android.phone">
            <intent-filer>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filer>
        </activity>
    </application>
</manifest> 

