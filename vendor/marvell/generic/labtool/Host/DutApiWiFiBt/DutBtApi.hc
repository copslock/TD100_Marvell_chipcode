// ACL 
//   DM1 = 0x01;   DM3 = 0x03;   DM5 = 0x05; (GFSK, 1M FEC)
//   DH1 = 0x11;   DH3 = 0x13;   DH5 = 0x15; (GFSK, 1M)
// 2-DH1 = 0x21; 2-DH3 = 0x23; 2-DH5 = 0x25; (DQPSK, 2M)
// 3-DH1 = 0x31; 3-DH3 = 0x33; 3-DH5 = 0x35; (8PSK, 3M)

// Sco:
// HV1 = 0x11;   HV2 = 0x12;   HV3 = 0x13; (GFSK, 1M)

// eSco:
//   EV3 = 0x13;   EV4 = 0x14;   EV5 = 0x15; (GFSK, 1M)
// 2-EV3 = 0x23; 2-EV5 = 0x25;		 (DQPSK, 2M)
// 3-EV3 = 0x33; 3-EV5 = 0x35;		 (8PSK, 3M)

enum
{
	DATARATE_1M_GFSK_FEC,
	DATARATE_1M_GFSK,	
	DATARATE_2M_DQPSK,
	DATARATE_3M_8PSK
};
enum
{
TIMESLOT_1SLOT=1,
TIMESLOT_2SLOT,
TIMESLOT_3SLOT,
TIMESLOT_4SLOT,
TIMESLOT_5SLOT 
};

