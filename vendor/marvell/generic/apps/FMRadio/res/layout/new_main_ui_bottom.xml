<LinearLayout  xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content"
    android:layout_height="125px"
    android:orientation="vertical"    
    android:background="@drawable/menu1"
    >
              
	<LinearLayout
	    android:orientation="horizontal"
	    android:layout_width="fill_parent"	    
	    android:layout_height="30px"
	    android:paddingLeft="35px"
	    android:paddingRight="40px"
	    android:layout_marginTop="30px"
	    android:layout_gravity="center_horizontal">
	    
	<Button
		android:id="@+id/new_btn_tune_left"
        android:layout_height="25px"
        android:layout_width="21px"
        android:layout_marginLeft="20px"
		android:background="@drawable/new_btn_tune_left"
    />
    
    <Button
		android:id="@+id/new_btn_scrol"
        android:layout_height="30px"
        android:layout_width="150px"
        android:layout_marginLeft="10px"
		android:background="@drawable/btn_scrol"
    />
    
    <Button
		android:id="@+id/new_btn_tune_right"
        android:layout_height="25px"
        android:layout_width="21px"
        android:layout_marginLeft="10px"
		android:background="@drawable/new_btn_tune_right"
    />
    </LinearLayout>
    <include layout="@layout/bottom2"/>
</LinearLayout>
