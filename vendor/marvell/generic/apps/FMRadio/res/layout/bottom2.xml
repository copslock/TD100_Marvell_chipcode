<LinearLayout  xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
    android:layout_height="100px"
    android:layout_marginTop="5px"
    android:orientation="horizontal"    
    >
              
	<Button
		android:id="@+id/new_btn_tune_prev"
        android:layout_height="50px"
        android:layout_width="50px"
        android:layout_marginLeft="20px"
		android:background="@drawable/btn_tune_prev"
    />
    	    
	<Button
		android:id="@+id/new_btn_list"
        android:layout_height="50px"
        android:layout_width="50px"
        android:layout_marginLeft="20px"
		android:background="@drawable/new_btn_list"
	/>
	<Button
		android:id="@+id/new_btn_add"
        android:layout_height="50px"
        android:layout_width="50px"
        android:layout_marginLeft="40px"
		android:background="@drawable/add"
    />

	<Button
		android:id="@+id/new_btn_tune_next"
        android:layout_height="50px"
        android:layout_width="50px"
        android:layout_marginLeft="20px"
        android:layout_alignParentRight="true"
        android:layout_marginRight="5px"
		android:background="@drawable/btn_tune_next"
    />
</LinearLayout>
	
