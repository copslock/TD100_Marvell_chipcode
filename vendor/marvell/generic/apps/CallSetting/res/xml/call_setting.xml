 <PreferenceScreen
	 xmlns:android="http://schemas.android.com/apk/res/android"
	 xmlns:callsetting="http://schemas.android.com/apk/res/com.marvell.callsetting"
	 android:title="@string/callsetting_app_name"
	 android:key="parent">
	 <com.marvell.callsetting.IconPreferenceScreen
		 callsetting:icon="@drawable/ic_black_list_setting"
		 android:title="@string/black_list_setting_title">
		 <intent
			 android:action="android.intent.action.MAIN"
			 android:targetPackage="com.android.phone"
			 android:targetClass="com.android.phone.VideoCallBlackListActivity" />
	 </com.marvell.callsetting.IconPreferenceScreen>
	 <com.marvell.callsetting.IconPreferenceScreen
		 callsetting:icon="@drawable/ic_call_forwarding_setting"
		 android:title="@string/call_forwarding_setting_title">
		 <intent
			 android:action="android.intent.action.MAIN"
			 android:targetPackage="com.marvell.callsetting"
			 android:targetClass="com.marvell.callsetting.CallForwardingActivity" />
	 </com.marvell.callsetting.IconPreferenceScreen>
	 <com.marvell.callsetting.IconPreferenceScreen
		 callsetting:icon="@drawable/ic_call_baring_setting"
		 android:title="@string/call_baring_setting_title">
		 <intent
			 android:action="android.intent.action.MAIN"
			 android:targetPackage="com.marvell.callsetting"
			 android:targetClass="com.marvell.callsetting.CallBarringActivity" />
	 </com.marvell.callsetting.IconPreferenceScreen>
	 <com.marvell.callsetting.IconPreferenceScreen
		 callsetting:icon="@drawable/ic_call_camera_setting"
		 android:title="@string/call_camera_settings_title">
		 <intent
			 android:action="android.intent.action.MAIN"
			 android:targetPackage="com.marvell.callsetting"
			 android:targetClass="com.marvell.callsetting.CallReplaceImageActivity" />
	 </com.marvell.callsetting.IconPreferenceScreen>
	 <com.marvell.callsetting.IconPreferenceScreen
		 callsetting:icon="@drawable/ic_call_fallback_setting"
		 android:title="@string/call_fallback_setting_title">
		 <intent
			 android:action="android.intent.action.MAIN"
			 android:targetPackage="com.android.phone"
			 android:targetClass="com.android.phone.VideoCallFallbackActivity" />
	 </com.marvell.callsetting.IconPreferenceScreen>
</PreferenceScreen>
