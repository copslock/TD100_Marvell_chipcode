 <PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
	 <PreferenceCategory
		 android:title="@string/voice_call_forward_category"/>
	 <PreferenceScreen
		 android:key="vc_forward"
		 android:title="@string/voice_call_forward_title"
		 android:summary="@string/voice_call_forward_summary"
		 android:persistent="false">
		 <intent android:action="android.intent.action.MAIN"
			 android:targetPackage="com.android.phone"
			 android:targetClass="com.android.phone.GsmUmtsCallForwardOptions"/>
	 </PreferenceScreen>
	 <PreferenceCategory
		 android:title="@string/video_call_forward_category"/>
	 <PreferenceScreen
		 android:key="vt_forward"
		 android:title="@string/video_call_forward_title"
		 android:summary="@string/video_call_forward_summary"
		 android:persistent="false">
		 <intent android:action="android.intent.action.MAIN"
			 android:targetPackage="com.android.phone"
			 android:targetClass="com.android.phone.VideoCallForwardingActivity"/>
	 </PreferenceScreen>
</PreferenceScreen>
