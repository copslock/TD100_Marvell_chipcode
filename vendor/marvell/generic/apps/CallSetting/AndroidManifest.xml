<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.marvell.callsetting"
    android:sharedUserId="android.uid.phone">
	<original-package android:name="com.marvell.callsetting"/>
	<application android:icon="@drawable/callsetting_app_icon"
		android:label="@string/callsetting_app_name">
		<activity android:name=".CallSettingActivity"
			android:label="@string/callsetting_app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".CallForwardingActivity"
			android:label="@string/voice_video_call_forward">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".CallReplaceImageActivity"
			android:label="@string/remote_settings_label">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".CallBarringActivity"
			android:label="@string/voice_video_call_barring">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
	</application>
</manifest> 
